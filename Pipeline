pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'This is Build Stage - First Stage'
            }
        }
    
      
        stage('Test') {
            steps {
                echo 'This is Test Stage - Second Stage'
            }
        }
    
     
      
        stage('Release') {
            steps {
                echo 'This is Release Stage - Third Stage'
            }
        }
    }
}
